   "source": [
    "def A(k,n):\n",
    "    result = 0\n",
    "    for i in range(0,k):\n",
    "        result += (-1)^(i)*AplusA(k-i,n)\n",
    "    return result + (-1)^(k)*h[n]"
   ]
